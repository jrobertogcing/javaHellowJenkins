                echo "Build"
                javac HelloWorld.java
            }
        }
        stage('Test') { 
            steps {
                // 
                echo "Test"
                java HelloWorld

            }
        }
        stage('Deploy') { 
            steps {
                // 
                echo "Deploy"
            }
        }
    }
}
